#!/usr/bin/env node

/**
 * Module dependencies.
 */
var Mocha = require('mocha');
var Q = require('q');
var app = require('../app');
var debug = require('debug')('jvent:server');
var http = require('http');

var mocha = new Mocha({
    // reporter: 'html'
});
mocha.addFile('./tests/index');

Q.all([
    require('../models/db')("mochatest")
])
.then(function() {
    mocha.run();
});